#include <stdio.h>

int main() {

    int n;

    scanf("%d", &n);

    int arr[n];

    for (int i = 0; i < n; i++) {

        scanf("%d", &arr[i]);

    }

    int positive_count = 0;

    int negative_count = 0;

    for (int i = 0; i < n; i++) {

        if (arr[i] > 0) {

            positive_count++;

        } else if (arr[i] < 0) {

            negative_count++;

        }

    }

    printf("Count of Positive Integer is %d.00\n", positive_count);

    printf("Count of Negative Integer is %d.00\n", negative_count);

    return 0;

}
