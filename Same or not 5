#include <stdio.h>

int main() {

    int n, num, even = 0, odd = 0;

    scanf("%d", &n);

    

    for(int i = 0; i < n; i++) {

        scanf("%d", &num);

        if(num % 2 == 0)

            even++;

        else

            odd++;

    }

    if(even == n)

        printf("Even\n");

    else if(odd == n)

        printf("Odd\n");

    else

        printf("Mixed\n");

    return 0;

}
