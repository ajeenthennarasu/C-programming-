#include <stdio.h>

int main() {

    int n, x, i, found = 0;

    scanf("%d", &n);

    int arr[n];

    for(i = 0; i < n; i++) {

        scanf("%d", &arr[i]);

    }

    scanf("%d", &x);

    for(i = 0; i < n; i++) {

        if(arr[i] == x) {

            found = 1;

            break;

        }

    }

    if(found) {

        printf("%d is presented in an array.\n", x);

    } else {

        printf("%d is not presented in an array.\n", x);

    }

    return 0;

}
